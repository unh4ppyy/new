task.spawn(function()
        while task.wait() do
                game:GetService("StarterGui"):SetCore("SendNotification", {
                    Title = "jew detector",
                    Text = "JEW DETECTED YOU WILL BE BANNED YE",
                    Duration = 3,
                })
        end
end)

wait(4)

game.Players.LocalPlayer:Kick("You have been banned")
